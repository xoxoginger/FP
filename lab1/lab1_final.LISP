(defun choose-in-1-3(x y z)
	 (cond 
          ((and (and (>= x 1) (<= x 3)) 
                (and (>= y 1) (<= y 3)) 
                (and (>= z 1) (<= z 3))) (values x y z)) 
          ((and (and (>= x 1) (<= x 3)) 
                (and (>= y 1) (<= y 3))
                (or (> z 3) (< z 1))) (values x y))
          ((and (and (>= x 1) (<= x 3)) 
                (and (>= z 1) (<= z 3))
                (or (> y 3) (< y 1))) (values x z))
          ((and (and (>= z 1) (<= z 3)) 
                (and (>= y 1) (<= y 3))
                (or (> x 3) (< x 1))) (values y z))
          ((and (and (>= x 1) (<= x 3)) 
                (or (> y 3) (< y 1)) 
                (or (> z 3) (< z 1))) (values x))
          ((and (and (>= y 1) (<= y 3)) 
                (or (> x 3) (< x 1)) 
                (or (> z 3) (< z 1))) (values y))
          ((and (and (>= z 1) (<= z 3)) 
                (or (> y 3) (< y 1)) 
                (or (> x 3) (< x 1))) (values z))
          ((and (or (> x 1) (< x 3)) 
                (or (> y 3) (< y 1)) 
                (or (> z 3) (< z 1))) (values))))
